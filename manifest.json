{
    "manifest_version": 3,
    "name": "YouTube Educational Filter",
    "version": "1.1",
    "description": "Show only educational content on YouTube",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Educational Filter"
    }
  }
  